{
  "name": "react-preloader-icon",
  "version": "0.1.1",
  "description": "SVG loading icons component for React.",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && mkdir dist",
    "build:browser": "webpack --config webpack.prod.js",
    "build:commonjs": "babel src -d dist --ignore browser.js --presets env,react --plugins transform-class-properties",
    "build:copyfile": "node copyfiles",
    "build": "npm run build:browser && npm run build:commonjs && npm run build:copyfile",
    "release": "npm publish dist",
    "deploy": "gh-pages -d dist -m 'Site updated'",
    "dev": "webpack-dev-server --open --progress --inline --config webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/UYEONG/react-preloader-icon.git"
  },
  "keywords": [
    "preloader",
    "svg",
    "loading",
    "icon"
  ],
  "author": "UYEONG <uyeong21c@gmail.com> (http://blog.coderifleman.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/UYEONG/react-preloader-icon/issues"
  },
  "homepage": "https://github.com/UYEONG/react-preloader-icon#readme",
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-loader": "7.1.4",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "expose-loader": "0.7.5",
    "fs-extra": "^5.0.0",
    "gh-pages": "1.1.0",
    "html-webpack-plugin": "3.1.0",
    "webpack": "4.3.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "3.1.1",
    "webpack-merge": "^4.1.2"
  },
  "dependencies": {
    "bezier-easing": "2.0.3",
    "object-assign": "4.1.1",
    "prop-types": "15.6.1",
    "react": "^16.2.0",
    "stepperjs": "^0.2.1"
  }
}
